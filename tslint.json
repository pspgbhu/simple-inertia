{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "no-boolean-literal-compare": false,
        "object-literal-sort-keys": false,
        "import-name": false,
        "no-console": false,
        "align": [
            true,
            "elements",
            "members",
            "parameters",
            "statements"
        ]
    },
    "rulesDirectory": []
}